{
    "defaultSeverity": "warning",
    "extends": [
      "tslint:recommended",
      "tslint-eslint-rules",
      "tslint-microsoft-contrib/latest",
      "tslint-consistent-codestyle",
      "tslint-config-prettier"
    ],
    "rules": {
        "no-conditional-assignment": true,
        "no-console": false,
        "completed-docs":false,
        "object-literal-shorthand":false
    }
}